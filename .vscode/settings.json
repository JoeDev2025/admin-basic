{
  "todo-tree.general.tags": ["TODO", "FIXME", "BUG", "NOTE", "REVIEW"],
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "check",
      "foreground": "#FFD700"
    },
    "FIXME": {
      "icon": "bug",
      "foreground": "#FF4C4C"
    },
    "REVIEW": {
      "icon": "flame",
      "foreground": "#f700ff"
    },
    "NOTE": {
      "icon": "note",
      "foreground": "#0984ff"
    }
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode"
}
